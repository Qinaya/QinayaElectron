{
  "$schema": "https://raw.githubusercontent.com/electron-userland/electron-builder/master/packages/app-builder-lib/scheme.json",
  "appId": "co.qinaya.cloudpro",
  "asar": true,
  "productName": "QinayaCloudPro",
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "dist",
    "dist-electron"
  ],
  "mac": {
  "icon": "./public/icon.icns",

    "target": [
      "dmg"
    ],
    "artifactName": "${productName}-Mac-${version}-Installer.${ext}",
    "publish": [
      {
        "provider": "github",
        "owner": "Qinaya",
        "repo": "QinayaElectron",
        "releaseType": "release"
      }
    ]
  },
"win": {
  "icon": "./public/iconl.ico",
  "publisherName": ["Qinaya"],
  "target": [{
    "target": "nsis",
  }],
  "artifactName": "${productName}-Windows-${version}-Setup.${ext}",
  "publish": [{
    "provider": "github",
    "owner": "Qinaya",
    "repo": "QinayaElectron",
    "releaseType": "release"
  }]
},
"nsis": {
  "oneClick": false,
  "perMachine": true,
  "allowElevation": true,
  "installerIcon": "./public/iconl.ico",
  "uninstallerIcon": "./public/iconl.ico",
  "installerHeaderIcon": "./public/iconl.ico",
  "artifactName": "${productName}-Windows-${version}-Setup.${ext}",
  "deleteAppDataOnUninstall": true,
  "createDesktopShortcut": "always",
  "menuCategory": "Qinaya",
},
"linux": {
  "icon": "./public/qinaya.png",
  "category": "Internet",
  "target": [
    {
      "target": "AppImage",
      "arch": ["x64", "armv7l", "arm64"]
    },
    {
      "target": "deb",
      "arch": ["x64", "armv7l", "arm64"]
    },
    {
      "target": "tar.gz",
      "arch": ["x64", "armv7l", "arm64"]
    },
    {
      "target": "snap"
    }
  ],
  "artifactName": "${productName}-Linux-${version}-${arch}.${ext}",
  "publish": [
    {
      "provider": "github",
      "owner": "Qinaya",
      "repo": "QinayaElectron",
      "publishAutoUpdate": true,
      "releaseType": "release"
    },
    {
      "provider": "snapStore",
      "repo": "QinayaCloudPro",
      "channels": ["stable"],
      "publishAutoUpdate": true,
      "timeout": 120000
    }
  ]
}

  }



     
